{
  "name": "angular2-tutorial",
  "version": "0.1.0",
  "description": "Angular2 tutorial sample application",
  "main": "",
  "scripts": {
    "clean": "npm cache clean && rimraf -- node_modules doc coverage dist build src/**/*.css",
    "clean:dist": "rimraf -- dist build",
    "preclean:install": "npm run clean",
    "clean:install": "npm set progress=false && npm install",
    "preclean:start": "npm run clean",
    "clean:start": "npm start",
    "build": "npm run build:dev",
    "prebuild:dev": "npm run clean:dist",
    "build:dev": "webpack --config config/webpack.dev.js --progress --profile --colors --display-error-details --display-cached",
    "prebuild:prod": "npm run clean:dist",
    "build:prod": "npm run sass && npm run ngc && webpack --config config/webpack.aot.js --progress --profile --bail",
    "server": "npm run server:dev",
    "server:dev": "webpack-dev-server --config config/webpack.dev.js --progress --profile --watch --content-base src/",
    "server:prod": "http-server dist --cors",
    "test": "npm run sass && karma start",
    "test:watch": "npm-run-all -p -r sass:watch karma:watch",
    "karma:watch": "karma start --browsers Chrome --reporters dots,notify --single-run false",
    "docs": "typedoc  --options typedoc.json  src/**/*.ts",
    "start": "npm-run-all -p -r sass:watch server:dev",
    "ngc": "ngc -p tsconfig.aot.json --locale=en-US",
    "presass": "rimraf -- src/**/*.css",
    "sass": "node-sass src -o src --include-path node_modules --output-style compressed -q",
    "sass:watch": "node-sass src -o src && node-sass -w src -o src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/springboot-angular2-tutorial/angular2-app.git"
  },
  "author": "akirasosa <akirasosa@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/springboot-angular2-tutorial/angular2-app/issues"
  },
  "homepage": "https://github.com/springboot-angular2-tutorial/angular2-app",
  "dependencies": {
    "@angular/common": "2.0.2",
    "@angular/compiler": "2.0.2",
    "@angular/core": "2.0.2",
    "@angular/forms": "2.0.2",
    "@angular/http": "2.0.2",
    "@angular/platform-browser": "2.0.2",
    "@angular/platform-browser-dynamic": "2.0.2",
    "@angular/router": "3.0.2",
    "bootstrap": "^4.0.0-alpha.4",
    "core-js": "^2.4.1",
    "jquery": "^2.2.4",
    "lodash-es": "^4.16.4",
    "md5-hex": "^1.1.0",
    "moment": "^2.12.0",
    "pluralize": "~3.0.0",
    "rxjs": "5.0.0-beta.12",
    "time-ago": "^0.1.0",
    "toastr": "^2.1.2",
    "zone.js": "~0.6.17"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^0.6.4",
    "@angular/platform-server": "^2.0.2",
    "@types/jasmine": "^2.5.35",
    "@types/lodash": "^4.14.37",
    "@types/node": "^6.0.45",
    "@types/pluralize": "0.0.27",
    "@types/source-map": "^0.1.28",
    "@types/toastr": "^2.1.27",
    "@types/webpack": "^1.12.35",
    "angular2-template-loader": "^0.5.0",
    "awesome-typescript-loader": "^2.2.4",
    "codelyzer": "~1.0.0-beta.0",
    "css-loader": "^0.25.0",
    "exports-loader": "^0.6.3",
    "jasmine-core": "^2.4.1",
    "karma": "^1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-jasmine": "~1.0.2",
    "karma-mocha-reporter": "^2.0.2",
    "karma-notify-reporter": "^1.0.0",
    "karma-phantomjs-launcher": "~1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "node-sass": "^3.5.3",
    "npm-run-all": "^3.1.0",
    "phantomjs-prebuilt": "^2.1.13",
    "raw-loader": "^0.5.1",
    "rimraf": "2.5.4",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.0",
    "ts-helpers": "^1.1.1",
    "tslint": "^3.15.1",
    "tslint-loader": "^2.1.3",
    "typescript": "^2.0.3",
    "webpack": "2.1.0-beta.25",
    "webpack-dev-server": "^2.1.0-beta.8",
    "webpack-manifest-plugin": "^1.0.0",
    "webpack-merge": "~0.14.1"
  }
}
