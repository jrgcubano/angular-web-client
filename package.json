{
  "name": "angular2-tutorial",
  "version": "0.1.0",
  "description": "Angular2 tutorial sample application",
  "main": "",
  "scripts": {
    "rimraf": "rimraf",
    "clean": "npm cache clean && npm run rimraf -- node_modules doc coverage dist",
    "clean:dist": "npm run rimraf -- dist",
    "preclean:install": "npm run clean",
    "clean:install": "npm set progress=false && npm install",
    "preclean:start": "npm run clean",
    "clean:start": "npm start",
    "build": "npm run build:dev",
    "prebuild:dev": "npm run clean:dist",
    "build:dev": "webpack --config config/webpack.dev.js --progress --profile --colors --display-error-details --display-cached",
    "prebuild:prod": "npm run clean:dist",
    "build:prod": "webpack --config config/webpack.prod.js  --progress --profile --colors --display-error-details --display-cached --bail",
    "server": "npm run server:dev",
    "server:dev": "webpack-dev-server --config config/webpack.dev.js --progress --profile --watch --content-base src/",
    "server:dev:hmr": "npm run server:dev -- --hot --inline ",
    "server:prod": "http-server dist --cors",
    "test": "node_modules/karma/bin/karma start",
    "docs": "typedoc  --options typedoc.json  src/**/*.ts",
    "start": "npm run server:dev",
    "start:hmr": "npm run server:dev:hmr"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/springboot-angular2-tutorial/angular2-app.git"
  },
  "author": "akirasosa <akirasosa@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/springboot-angular2-tutorial/angular2-app/issues"
  },
  "homepage": "https://github.com/springboot-angular2-tutorial/angular2-app",
  "dependencies": {
    "@angular/common": "2.0.0",
    "@angular/compiler": "2.0.0",
    "@angular/core": "2.0.0",
    "@angular/forms": "^2.0.0",
    "@angular/http": "2.0.0",
    "@angular/platform-browser": "2.0.0",
    "@angular/platform-browser-dynamic": "2.0.0",
    "@angular/router": "3.0.0",
    "core-js": "^2.4.1",
    "jquery": "^2.2.4",
    "lodash": "^4.14.0",
    "md5-hex": "^1.1.0",
    "moment": "^2.12.0",
    "pluralize": "~3.0.0",
    "rxjs": "5.0.0-beta.12",
    "time-ago": "^0.1.0",
    "toastr": "^2.1.2",
    "zone.js": "~0.6.17"
  },
  "devDependencies": {
    "@types/jasmine": "^2.2.31",
    "@types/lodash": "^4.14.36",
    "@types/node": "^6.0.41",
    "@types/pluralize": "0.0.27",
    "@types/source-map": "^0.1.27",
    "@types/toastr": "^2.1.27",
    "@types/webpack": "^1.12.31",
    "angular2-hmr": "~0.8.1",
    "angular2-template-loader": "^0.5.0",
    "awesome-typescript-loader": "^2.2.4",
    "bootstrap-loader": "^1.0.8",
    "bootstrap-sass": "^3.3.7",
    "codelyzer": "~0.0.26",
    "css-loader": "^0.23.1",
    "exports-loader": "^0.6.3",
    "expose-loader": "^0.7.1",
    "file-loader": "^0.9.0",
    "imports-loader": "^0.6.5",
    "jasmine-core": "^2.4.1",
    "karma": "^1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-jasmine": "~1.0.2",
    "karma-mocha-reporter": "^2.0.2",
    "karma-notify-reporter": "^1.0.0",
    "karma-phantomjs-launcher": "1.0.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "node-sass": "^3.5.3",
    "phantomjs-prebuilt": "^2.1.8",
    "raw-loader": "^0.5.1",
    "resolve-url-loader": "~1.6.0",
    "rimraf": "2.5.4",
    "sass-loader": "~4.0.0",
    "source-map-loader": "^0.1.5",
    "string-replace-loader": "1.0.5",
    "style-loader": "^0.13.0",
    "ts-helpers": "^1.1.1",
    "tslint": "^3.15.1",
    "tslint-loader": "^2.1.3",
    "typescript": "^2.0.0",
    "url-loader": "^0.5.7",
    "webpack": "2.1.0-beta.22",
    "webpack-dev-server": "^2.1.0-beta.2",
    "webpack-manifest-plugin": "^1.0.0",
    "webpack-merge": "~0.14.1"
  }
}
